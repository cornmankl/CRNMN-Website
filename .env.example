# AI Service Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
VITE_GLM_API_KEY=your_glm_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Firebase Configuration (for image uploads)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Payment Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Google APIs (Optional)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key

# External Services (Optional)
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_access_key

# AI Configuration
VITE_AI_DEFAULT_MODEL=gemini-pro
VITE_AI_MAX_TOKENS=2000
VITE_AI_TEMPERATURE=0.7
VITE_AI_ENABLE_IMAGE_GENERATION=true
VITE_AI_ENABLE_WEBSITE_MODIFICATION=true

# Rate Limiting
VITE_AI_RATE_LIMIT_CHAT_USER=50
VITE_AI_RATE_LIMIT_CHAT_PREMIUM=200
VITE_AI_RATE_LIMIT_CHAT_ADMIN=1000
VITE_AI_RATE_LIMIT_IMAGE_USER=5
VITE_AI_RATE_LIMIT_IMAGE_PREMIUM=50
VITE_AI_RATE_LIMIT_IMAGE_ADMIN=200