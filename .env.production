# Production environment variables
# Add your actual production keys here

# AI Service Configuration
VITE_GEMINI_API_KEY=your_production_gemini_api_key_here
VITE_OPENAI_API_KEY=your_production_openai_api_key_here
VITE_ANTHROPIC_API_KEY=your_production_anthropic_api_key_here
VITE_GLM_API_KEY=your_production_glm_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_production_supabase_project_url
