VITE_AI_RATE_LIMIT_CHAT_USER=50
VITE_AI_RATE_LIMIT_CHAT_PREMIUM=200
VITE_AI_RATE_LIMIT_CHAT_ADMIN=1000
VITE_AI_RATE_LIMIT_IMAGE_USER=5
VITE_AI_RATE_LIMIT_IMAGE_PREMIUM=50
VITE_AI_RATE_LIMIT_IMAGE_ADMIN=200